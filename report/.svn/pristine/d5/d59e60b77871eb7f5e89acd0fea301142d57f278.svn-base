package com.jshuabo.reportcenter.server.utils.list;

import java.util.ArrayList;
import java.util.List;

/**
 * @ClassName: ListUtils
 * @Description: 
 * @author: peng.wu
 * @date: 2014年10月23日 下午3:33:28
 */
public class ListUtils {
    
    /**
     * @Title: StringToLongList String list to Long list
     * @param t
     * @return: List<Long>
     * @date: 2014年10月23日 下午3:38:14
     */
    public static List<Long> stringToLongList(List<String> t){
        List<Long> lls = new ArrayList<Long>();
        for (String str : t) {
            lls.add(Long.valueOf(str));
        }
        return lls;
    }

}
